{
  "name": "aetasaal-api",
  "private": true,
  "version": "1.0.0",
  "description": "Aetasaal API",
  "scripts": {
    "build-ts": "tsc",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "nodemon server --node-env=development | bunyan -o simple",
    "start:dev": "nodemon server --node-env=development | bunyan -o simple",
    "start:qa": "nodemon server --node-env=qa | bunyan -o simple",
    "watch": "nodemon server | bunyan -o simple",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "pretest": "npm run lint",
    "sqlz:createdb": "sequelize db:create",
    "sqlz:migrate": "sequelize db:migrate",
    "sqlz:undo": "sequelize db:migrate:undo",
    "sqlz:new": "sequelize migration:create",
    "sqlz:seednew": "sequelize seed:create",
    "sqlz:seed": "sequelize db:seed:all",
    "sqlz:seedp": "sequelize db:seed --seed"
  },
  "author": "Muhammad Owais <mok.developer@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/cli": "^17.2.0",
    "@koa/router": "^12.0.1",
    "@sendgrid/mail": "8.1.1",
    "@types/koa-bunyan-logger": "^2.1.7",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-static": "^4.0.4",
    "boom": "7.2.0",
    "bunyan": "^1.8.15",
    "cloudinary": "^2.0.1",
    "convict": "6.2.4",
    "dotenv": "^16.4.4",
    "fcm-push": "^1.1.3",
    "fs-extra": "^11.2.0",
    "joi": "17.12.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "4.0.0",
    "koa": "^2.15.0",
    "koa-body": "6.0.1",
    "koa-bunyan-logger": "2.1.0",
    "koa-compose": "^4.1.0",
    "koa-helmet": "7.0.2",
    "koa-mount": "4.0.0",
    "koa-router": "^12.0.1",
    "koa-static": "5.0.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.9",
    "pg": "^8.11.3",
    "pg-hstore": "2.3.4",
    "randexp": "git://github.com/fent/randexp.js.git#v0.5.2",
    "sequelize": "^6.37.0",
    "sequelize-cli": "6.6.2",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@types/boom": "^7.3.5",
    "@types/bunyan": "1.8.11",
    "@types/convict": "6.1.6",
    "@types/dotenv": "6.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^4.0.0",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jwt-decode": "2.2.1",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-compose": "3.2.8",
    "@types/koa-helmet": "6.0.8",
    "@types/koa-router": "^7.4.8",
    "@types/koa2-cors": "^2.0.5",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "6.4.14",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "9.0.8",
    "jest": "29.7.0",
    "loadash": "^1.0.0",
    "nodemon": "3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "29.1.2",
    "ts-node": "^10.9.2",
    "tslint": "5.20.1",
    "typescript": "^5.3.3"
  },
  "engineStrict": true,
  "engines": {
    "node": "10.2.1"
  }
}
